.MODEL small
.STACK 200h
.DATA
Buffer DB 6, ?, 6 dup (?)
A DW ?
B DW ?
C DW ?
Str1 DB 6 dup (?)
.CODE
ProgramStart:   
    mov AX, @DATA
    mov DS, AX
    
    mov AH, 0Ah ;Piepras?t Astr
    mov DX, offset Buffer
    int 21h
    
 mov CL, Buffer[1]
 mov SI, 2
 
LoopStart:

 mov AL, 10
 mul A
 mov A, AX
 mov BL, Buffer[SI]
 add A, BX
 sub A, 48

 add SI, 1
 sub CL, 1

 cmp CL, 0 
 jne LoopStart;
   mov CX, A
    
    
    mov AH, 0Fh ;Clear
    int 10h
    mov AH, 0
    int 10h
    
    mov AH, 0Ah ;Piepras?t Bstr
    mov DX, offset Buffer
    int 21h
    
 mov CL, Buffer[1]
 mov SI, 2
 
LoopStart1:

 mov AL, 10
 mul B
 mov B, AX
 mov BL, Buffer[SI]
 add B, BX
 sub B, 48

 add SI, 1
 sub CL, 1

 cmp CL, 0 
 jne LoopStart1
 mov CX, B
 
    fild A
    fild B
    fadd 
    fistp C
 
    mov BL, CL ; to BX
    mov BH, 0
    mov AX, C
    mov SI, BX
    mov str1[SI], '$'
String:
    sub SI, 1
    mov AH, 0
    mov BH, 10
    div BH
    mov BH, AH
    add BH, 48
    mov str1[SI], BH
    cmp AL, 0
    jne String
    
    mov DL, 10
    mov AH, 02h
    int 21h
    
    mov AH, 9 ; Output
    mov DX, offset str1
    int 21h
    
    mov AH, 0
    int 16h
                
    mov AH, 4Ch
    int 21h
end ProgramStart 
